{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {
    "config": "^3.3.9",
    "dotenv": "^16.1.4",
    "firebase": "^9.22.2",
    "firebase-tools": "^12.3.1",
    "global": "^4.4.0",
    "jest": "^29.5.0",
    "mongoose": "^7.2.4",
    "next": "^13.4.5",
    "react-firebase-hooks": "^5.1.1",
    "react-hooks": "^1.0.1",
    "turbo": "1.8.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "eslint-config-custom": "*",
    "prettier": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
